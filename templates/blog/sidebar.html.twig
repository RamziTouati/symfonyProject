 <div class="row post mt-4">
     <div class="col-12">
        <h2 class="text-capitalize">Article recents</h2>
        <hr>
        <div class="row post mb-1">
            {% for post in latests %}
                <div class="col-12">
                    <h4 class="latest"><a href="{{ path('blog_show', {'id': post.id}) }}" class="username">{{post.title}}</a></h4>
                </div>
            {% endfor %}
        </div>
    </div>
 </div>